{
  "expo": {
    "name": "Texty",
    "slug": "Texty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/texty-icon-iOS-Default-1024x1024@1x.png",
    "scheme": "texty",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kirills.chatapp",
      "associatedDomains": [
        "applinks:relative-fly-62.clerk.accounts.dev",
        "webcredentials:relative-fly-62.clerk.accounts.dev"
      ],
      "icon": "./assets/texty-icon-iOS-Default-1024x1024@1x.png",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kirills.chatapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": { "deploymentTarget": "16.0" },
          "android": { "compileSdkVersion": 34, "targetSdkVersion": 34 }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
